	xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified">
	<List items="{templates}">
		<CustomListItem>
			<content>
				<HBox height="50px" justifyContent="SpaceBetween" alignItems="Center">
					<Link class="myText" text="{name}" />
